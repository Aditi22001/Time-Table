from random import shuffle
import numpy

matrix=numpy.zeros((6,9),dtype=numpy.int)
Lname=[]
name="Name"

def Class():
    for i in range(0,6):
        Lname.append((name+`i+1`))
    Lname.append('LAB')
    shuffle(Lname)
    count=0
    for i in Lname:
        if i=='LAB':
            print i
            count+=3
        elif count!=9:
            print i
            count+=1
        else:   
            print "\n"
            count=0
Class()


